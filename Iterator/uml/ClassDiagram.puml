@startuml

'Iterator UML class diagram'
'By Nika Chkhartishvili'

title Iterator

interface Iterator {
    +first() : void
    +next() : void
    +isDone() : void
    +currentItem() : Object
}

interface Iterable {
    +createIterator() : Iterator
}

class Destinations {
    +add(String value) : void
    -destinations : ArrayList<String>
}

class DestinationsIterator {
    -destinations : ArrayList<String>
    -position : int
}

Iterable <|-- Destinations
Iterator <|-- DestinationsIterator

@enduml